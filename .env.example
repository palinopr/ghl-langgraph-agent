# OpenAI
OPENAI_API_KEY=your-openai-api-key-here

# GoHighLevel
GHL_API_TOKEN=your-ghl-api-token-here
GHL_LOCATION_ID=your-ghl-location-id-here
GHL_CALENDAR_ID=your-ghl-calendar-id-here
GHL_ASSIGNED_USER_ID=your-ghl-assigned-user-id-here

# Supabase
SUPABASE_URL=your-supabase-url-here
SUPABASE_KEY=your-supabase-service-role-key-here

# LangSmith (optional but recommended)
LANGSMITH_TRACING=true
LANGSMITH_ENDPOINT=https://api.smith.langchain.com
LANGSMITH_API_KEY=your-langsmith-api-key-here
LANGSMITH_PROJECT=ghl-langgraph-agent
# Legacy support
LANGCHAIN_TRACING_V2=true
LANGCHAIN_API_KEY=your-langsmith-api-key-here
LANGCHAIN_PROJECT=ghl-langgraph-agent

# App Config
APP_ENV=development
LOG_LEVEL=INFO
PORT=8000

# OpenTelemetry (optional)
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4317

# Redis (optional for message batching)
REDIS_URL=redis://localhost:6379/0

# Enhanced Features (optional)
ENABLE_STREAMING=true
ENABLE_PARALLEL_CHECKS=true
ENABLE_MESSAGE_BATCHING=true
BATCH_WINDOW_SECONDS=15
MAX_BATCH_SIZE=10

# Model Configuration
OPENAI_MODEL=gpt-4-turbo
STREAMING_ENABLED=true
MAX_TOKENS_PER_MESSAGE=4000